Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"nGC4", "[", "g_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "adjmatrix", ",", "matrix4", ",", " ", "amountResult", ",", 
       "sumatoria"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"adjmatrix", "=", 
       RowBox[{"AdjacencyMatrix", "[", "g", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"matrix4", "=", 
       RowBox[{"MatrixPower", "[", 
        RowBox[{"adjmatrix", ",", "4"}], "]"}]}], ";", 
      RowBox[{"sumatoria", "=", 
       RowBox[{"Sum", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"VertexDegree", "[", 
            RowBox[{"g", ",", "i"}], "]"}], "*", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"VertexDegree", "[", 
              RowBox[{"g", ",", "i"}], "]"}], "-", "1"}], ")"}]}], ")"}], ",", 
