        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"matrix3", "[", 
            RowBox[{"[", "i", "]"}], "]"}], "[", 
           RowBox[{"[", "i", "]"}], "]"}], "*", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"VertexDegree", "[", 
             RowBox[{"g", ",", "i"}], "]"}], "-", "2"}], ")"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", 
           RowBox[{"VertexCount", "[", "g", "]"}]}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"amountResult", "=", 
       RowBox[{
        RowBox[{"(", 
