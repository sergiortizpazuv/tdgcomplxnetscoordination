  RowBox[{
   RowBox[{"nGC5", "[", "g_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "adjmatrix", ",", "matrix3", ",", "matrix5", ",", " ", "amountResult", 
       ",", "sumatoria"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"adjmatrix", "=", 
       RowBox[{"AdjacencyMatrix", "[", "g", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"matrix3", "=", 
       RowBox[{"MatrixPower", "[", 
        RowBox[{"adjmatrix", ",", "3"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"matrix5", "=", 
