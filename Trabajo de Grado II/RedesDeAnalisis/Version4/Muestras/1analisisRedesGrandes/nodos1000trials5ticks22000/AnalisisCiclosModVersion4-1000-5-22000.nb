 RowBox[{
  RowBox[{
   RowBox[{"nGC5", "[", "g_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "adjmatrix", ",", "matrix3", ",", "matrix5", ",", " ", "amountResult", 
       ",", "sumatoria"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"adjmatrix", "=", 
       RowBox[{"AdjacencyMatrix", "[", "g", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"matrix3", "=", 
       RowBox[{"MatrixPower", "[", 
        RowBox[{"adjmatrix", ",", "3"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"matrix5", "=", 
       RowBox[{"MatrixPower", "[", 
        RowBox[{"adjmatrix", ",", "5"}], "]"}]}], ";", 
      RowBox[{"sumatoria", "=", 
       RowBox[{"Sum", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"matrix3", "[", 
            RowBox[{"[", "i", "]"}], "]"}], "[", 
           RowBox[{"[", "i", "]"}], "]"}], "*", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"VertexDegree", "[", 
             RowBox[{"g", ",", "i"}], "]"}], "-", "2"}], ")"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", 
           RowBox[{"VertexCount", "[", "g", "]"}]}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"amountResult", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Tr", "[", "matrix5", "]"}], "-", 
          RowBox[{"(", 
           RowBox[{"5", "*", "sumatoria"}], ")"}], "-", 
          RowBox[{"(", 
           RowBox[{"5", "*", 
            RowBox[{"Tr", "[", "matrix3", "]"}]}], ")"}]}], ")"}], "/", 
        "10"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "amountResult", "]"}]}]}], "\[IndentingNewLine]",
