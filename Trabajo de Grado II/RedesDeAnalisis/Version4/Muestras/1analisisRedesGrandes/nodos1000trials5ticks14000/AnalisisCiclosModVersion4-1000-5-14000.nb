Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"nGC4", "[", "g_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "adjmatrix", ",", "matrix4", ",", " ", "amountResult", ",", 
       "sumatoria"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"adjmatrix", "=", 
       RowBox[{"AdjacencyMatrix", "[", "g", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"matrix4", "=", 
       RowBox[{"MatrixPower", "[", 
        RowBox[{"adjmatrix", ",", "4"}], "]"}]}], ";", 
      RowBox[{"sumatoria", "=", 
       RowBox[{"Sum", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"VertexDegree", "[", 
            RowBox[{"g", ",", "i"}], "]"}], "*", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"VertexDegree", "[", 
              RowBox[{"g", ",", "i"}], "]"}], "-", "1"}], ")"}]}], ")"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", 
           RowBox[{"VertexCount", "[", "g", "]"}]}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"amountResult", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Tr", "[", "matrix4", "]"}], "-", 
          RowBox[{"(", 
           RowBox[{"2", "*", "sumatoria"}], ")"}], "-", 
          RowBox[{"(", 
           RowBox[{"2", "*", 
            RowBox[{"EdgeCount", "[", "g", "]"}]}], ")"}]}], ")"}], "/", 
        "8"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "amountResult", "]"}]}]}], "\[IndentingNewLine]",
     "]"}]}], ";"}]], "Input",ExpressionUUID->"14516bca-1239-4142-90eb-\
